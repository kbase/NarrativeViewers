{
      "name" : "View Growth Curves",
      "ver" : "1.0.10",
      "authors" : [ ],
      "contact" : "help@kbase.us",
      "visble" : true,
      "categories" : ["inactive","metabolic_modeling"],
      "app_type" : "viewer",
      "widgets" : {
        "input" : "kbaseGrowthCurvesInput",
        "output" : "kbaseGrowthCurves"
      },
      "parameters" : [ {
        "id" : "input_growth_matrix",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
          "valid_ws_types" : [ "KBaseEnigmaMetals.GrowthMatrix" ]
        }
      },{
        "id" : "input_value_type",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "Series" ],
        "field_type" : "dropdown",
        "dropdown_options":{
          "options": [
            {
              "value": "Series",
              "display": "Series - grouped samples"
            },
            {
              "value": "Samples",
              "display": "Individual samples"
            }
          ] 
        }
    },{        
        "id" : "input_condition_ids",
        "optional" : true,
        "advanced" : false,
        "allow_multiple" : true,
        "default_values" : [ "" ],
        "field_type" : "custom_textsubdata",
        "dropdown_options":{
            "options": [
            ]
        }
      },{        
        "id": "input_show_error_bar",
        "optional":false,
        "advanced":false,
        "allow_multiple":false,
        "default_values":["1"],
        "field_type" : "checkbox",
        "checkbox_options":{
          "checked_value": 1,
          "unchecked_value": 0
        }
    },{
        "id": "input_log_scale",
        "optional":false,
        "advanced":false,
        "allow_multiple":false,
        "default_values":["1"],
        "field_type" : "checkbox",
        "checkbox_options":{
          "checked_value": 1,
          "unchecked_value": 0
        }
    }               
],
  "behavior" : {
    "none" : {
      "output_mapping" : [
        {
          "input_parameter": "input_growth_matrix",
          "target_property": "matrixID"
        },
        {
          "input_parameter": "input_value_type",
          "target_property": "valueType"            
        },
        {
          "input_parameter": "input_condition_ids",
          "target_property": "sampleSeriesIds"            
        },
        {
          "input_parameter": "input_show_error_bar",
          "target_property": "showErrorBar"
        },
        {
          "input_parameter": "input_log_scale",
          "target_property": "logScale"
        },          
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspaceID"
        }
      ]
    }
  }
}
